<h1 class="title">Arrival Report of <%="#{@employee.first_name} #{@employee.last_name}"%></h1>
<div class="row">
  <div class="col-xs-3">
    <label>From</label>
    <div class="input-group date">
      <input type="text" class="form-control" id="from">
      <div class="input-group-addon">
        <span class="fa fa-calendar"></span>
      </div>
    </div>
  </div>
  <div class="col-xs-3">
    <label>To</label>
    <div class="input-group date">
      <input type="text" class="form-control" id="to">
      <div class="input-group-addon">
        <span class="fa fa-calendar"></span>
      </div>
    </div>
  </div>
</div>
<div id="tbl_section"></div>
<input type="hidden" name="employee_id" id="employee_id" value="<%=@employee.id%>">
<%=javascript_include_tag 'arrival_time_report' %>
